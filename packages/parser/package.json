{
  "name": "@sveltosis/parser",
  "version": "0.1.0",
  "type": "module",
  "description": "Write svelte components once and compile to any framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "devDependencies": {
    "@builder.io/mitosis": "^0.0.64",
    "@sachinraja/eslint-config": "0.1.1",
    "@types/css-tree": "^1.0.7",
    "@types/lodash": "^4.14.185",
    "@types/node": "18.7.14",
    "astring": "^1.8.3",
    "dprint": "0.31.1",
    "eslint": "8.23.0",
    "husky": "8.0.1",
    "lodash": "^4.17.21",
    "module": "^1.2.5",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "svelte-preprocess": "^4.10.7",
    "tsup": "6.2.3",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vitest": "0.23.0"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "clean": true
  },
  "packageManager": "pnpm@7.11.0",
  "peerDependencies": {
    "svelte": "^3.50.0"
  },
  "dependencies": {
    "css-tree": "^2.2.1"
  }
}

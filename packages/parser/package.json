{
  "name": "@sveltosis/parser",
  "version": "0.6.1",
  "type": "module",
  "description": "Write svelte components once and compile to any framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "run-p lint:*",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "devDependencies": {
    "@babel/generator": "^7.19.4",
    "@babel/parser": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/traverse": "^7.19.4",
    "@babel/types": "^7.19.4",
    "@builder.io/mitosis": "^0.0.72",
    "@builder.io/mitosis-cli": "^0.0.32-6",
    "@sachinraja/eslint-config": "0.1.1",
    "@types/babel__core": "^7.1.19",
    "@types/estree": "^1.0.0",
    "@types/lodash": "^4.14.185",
    "@types/node": "18.7.14",
    "astring": "^1.8.3",
    "coffeescript": "^2.7.0",
    "eslint": "8.23.0",
    "husky": "8.0.1",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "module": "^1.2.5",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.16",
    "pug": "^3.0.2",
    "stylus": "^0.59.0",
    "sugarss": "^4.0.1",
    "tsup": "6.2.3",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vitest": "0.23.0"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "clean": true,
    "replaceNodeEnv": false
  },
  "peerDependencies": {
    "svelte": "^3.50.0",
    "svelte-preprocess": "^4.10.7"
  }
}
